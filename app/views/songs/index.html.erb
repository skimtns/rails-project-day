<h1>  The Songs on Billboard <%=@billboard.name %> </h1>
<h3> Rank and Song </h3>

<div class="ui bulleted list">
<ul>
  <% @songs.each do |song| %>
    <div class="item"> 
    <h4 class="ui teal header">
    <li>
     <i class="music icon"></i>
     <%= song.rank %> <%= song.name %>
     </li>
    </h4>
    </div>
  <% end %>
  </ul>
</div>